{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-undef": 0,
    "no-console": 0,
    "no-unused-vars": [1, {"args": "none"}],
    "no-mixed-spaces-and-tabs": 1,
    "brace-style": [1, "1tbs"],
    "comma-dangle": 0,
    "consistent-return": 1,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 1,
    "eqeqeq": [1, "allow-null"],
    "no-bitwise": 0,
    "no-dupe-class-members": 2,
    "no-multi-spaces": 1,
    "no-restricted-syntax": [2, "WithStatement"],
    "no-shadow": 2,
    "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true }],
    "keyword-spacing": 1,
    "space-before-blocks": 1,
    "react/prop-types": 1
  }

}
